import java.util.Random;

public class Switch {
    public static void main(String[] args) {

        Random i = new Random();
        int randomNumber = i.nextInt(10);

        System.out.println(randomNumber);

        switch (randomNumber) {

            case 1:
                System.out.println("one");
                break;
            case 2:
                System.out.println("two");
                break;
            case 3:
                System.out.println("three");
                break;
            case 4:
                System.out.println("four");
                break;
            case 5:
                System.out.println("five");
                break;
            case 6:
                System.out.println("six");
                break;
            case 7:
                System.out.println("seven");
                break;
            case 8:
                System.out.println("eight");
                break;
            case 9:
                System.out.println("nine");
                break;
        }

        switch (randomNumber) {

            case 1:

            case 2:

            case 3:

            case 4:
                System.out.println("lower like a five");
                break;
            case 5:
                System.out.println("the middle");
                break;
            case 6:

            case 7:

            case 8:

            case 9:
                System.out.println("bigger like a five");
                break;
        }

    }
}
